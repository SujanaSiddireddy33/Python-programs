##program to perform 2 to 10 right shift operations

print
a=int(input("Enter the digit: "))
print("right shift =",a>>2)
print("right shift =",a>>3)
print("right shift =",a>>4)
print("right shift =",a>>5)
print("right shift =",a>>6)
print("right shift =",a>>7)
print("right shift =",a>>8)
print("right shift =",a>>9)
print("right shift =",a>>10)
